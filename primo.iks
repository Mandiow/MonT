/* primos */


int main ()
{
	int valor;
	int aux <= 2;
	int quociente;
	int resto;
	int primo <= 1;
	while((aux < valor) && (valor != 0))
	do
	{
		quociente = valor/aux;
		resto = valor - quociente*aux;
		if(resto == 0)
		then
			{
				output "Não é primo";
				primo = 0;
			};
		aux = aux + 1
	};
	if(primo == 0)
	then
		{
			output "É primo";
		}
}
